@model PMQLSV.Models.Teacher

@{
    ViewData["Title"] = "TeacherPage";
}

<h1>TeacherPage</h1>

<div>
    <h4>Teacher Infomation</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Full Name
        </dt>
        <dd class="col-sm-10">
            @Html.Partial("_FullName", Model.User)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.User)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.User.Email)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User.Id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Classes)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Classes)
        </dd>
    </dl>
</div>

